			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="queryable" type="tns:boolean"
			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="operators" type="tns:string"
			    minOccurs="1" maxOccurs="unbounded" /> 
			<xs:element name="dataType" type="tns:string"
			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="updatable" type="tns:boolean"
			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="propertyFilterConfigurationInfo" type="tns:PropertyFilterConfigurationInfoType" 
					minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="PropertyFilterConfigurationInfoType">
		<xs:sequence>
			<xs:element name="operation" type="tns:enumPropertyFilterOperation" 
			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="isAllAllowed" type="tns:boolean"
			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="isNoneAllowed" type="tns:boolean"
			    minOccurs="1" maxOccurs="1" /> 
			<xs:element name="isListAllowed" type="tns:boolean"
			    minOccurs="1" maxOccurs="1" /> 
		</xs:sequence>
	</xs:complexType>
	
	<!-- query Operation -->
	<xs:element name="query">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="repositoryId" type="tns:ObjectIdType"
					minOccurs="1" maxOccurs="1" />
				<xs:element name="principalId" type="tns:PrincipalIdType" 
					minOccurs="1" maxOccurs="1" />
				<xs:element name="target" type="tns:QueryableObjectType" 
					minOccurs="1" maxOccurs="1" />
				<xs:element name="filter" type="tns:PropertyFilterType" 
					minOccurs="1" maxOccurs="1" />
				<xs:element name="criteria" type="tns:QueryConditionType" 
					minOccurs="1" maxOccurs="unbounded" />
				<xs:element name="navigationLimits" type="tns:NavigationConditionInfoType" 
					minOccurs="0" maxOccurs="1" />
				<xs:element name="maxItems" type="tns:integer" 
					minOccurs="0" maxOccurs="1" />
				<xs:element name="skipCount" type="tns:integer" 
					minOccurs="0" maxOccurs="1" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:complexType name="NavigationConditionInfoType">
		<xs:sequence>
			<xs:element name="parentNodeId" type="tns:ObjectIdType" 
				minOccurs="0" maxOccurs="1" />
			<xs:element name="limitToChildren" type="tns:boolean" 
				minOccurs="0" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:element name="queryResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="object" type="tns:PagingResponseType"
					minOccurs="1" maxOccurs="1" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<!-- Annotazioni -->
	<xs:complexType name="AnnotazioniPropertiesType">
		<xs:complexContent>
			<xs:extension base="tns:CommonPropertiesType">
				<xs:sequence>
					<xs:element name="objectTypeId" type="tns:enumCommonObjectType"/>
					<xs:element name="descrizione" type="tns:string"
						minOccurs="1" maxOccurs="1" /> 
					<xs:element name="idUtente" type="tns:IdUtenteType"
						minOccurs="1" maxOccurs="1" /> 
					<xs:element name="data" type="tns:date"
						minOccurs="1" maxOccurs="1" /> 
					<xs:element name="annotazioneFormale" type="tns:boolean"
						minOccurs="1" maxOccurs="1" /> 
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="ProtocolloPropertiesType">
		<xs:complexContent>
			<xs:extension base="tns:CommonPropertiesType">
				<xs:sequence>
					<xs:element name="objectTypeId" 					type="tns:enumCommonObjectType"/>
					<xs:element name="numRegistrazioneProtocollo"		type="tns:string"/>
					<xs:element name="dataRegistrazioneProtocollo"		type="tns:date"/>
					<xs:element name="oggetto"							type="tns:string"/>
					<xs:element name="entrata"							type="tns:boolean"/>
					<xs:element name="AOOCheRegistra"					type="tns:string"/>
					<xs:element name="EnteCheRegistra"					type="tns:string"/>
					<xs:element name="mittente"							type="tns:string" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="destinatario"						type="tns:string" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="segnaturaInternaXML"				type="tns:XMLType"/>
					<xs:element name="segnaturaEsternaXML"				type="tns:XMLType"/>
					<xs:element name="riservato"						type="tns:boolean"/>
					<xs:element name="annullato"						type="tns:boolean"/>
					<xs:element name="uuidRegistrazioneProtocollo"		type="tns:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	
	<xs:simpleType name="enumCommonObjectType">
		<xs:annotation>
			<xs:documentation>
				Oggetti di Common 
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="tns:string">
			<xs:enumeration value="AnnotazioniPropertiesType"/>
			<xs:enumeration value="ProtocolloPropertiesType"/>
		</xs:restriction>
	</xs:simpleType>
	
	<!-- getPropertiesMassive Operation -->
	<xs:element name="getPropertiesMassive">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="repositoryId" type="tns:ObjectIdType"
					minOccurs="1" maxOccurs="1" />
				<xs:element name="principalId" type="tns:PrincipalIdType" 
					minOccurs="1" maxOccurs="1" />
				<xs:element name="identifiers" type="tns:ObjectIdType" 
					minOccurs="1" maxOccurs="unbounded" />
				<xs:element name="filter" type="tns:PropertyFilterType" 
					minOccurs="1" maxOccurs="1" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="getPropertiesMassiveResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="object" type="tns:ObjectResponseType"
					minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>